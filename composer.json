{
    "name": "irfan-chowdhury/version-elevate",
    "description": "This package will notify the buyers about the new version and it can upgrade their current app to the new version.",
    "keywords": [
        "auto-update",
        "version auto update",
        "new version notify"
    ],
    "type": "library",
    "homepage": "https://github.com/Irfan-Chowdhury/version-elevate",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "IrfanChowdhury\\VersionElevate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IrfanChowdhury\\VersionElevate\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Irfan Chowdhury",
            "email": "irfanchowdhury80@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "IrfanChowdhury\\VersionElevate\\VersionElevateServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "8.x-dev",
        "pestphp/pest": "1.x-dev",
        "laravel/pint": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    }
}
